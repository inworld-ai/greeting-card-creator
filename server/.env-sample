# DO NOT EDIT THIS FILE DIRECTLY
# Copy this file to .env and enter your own values

# Required - Inworld API key (base64 encoded)
INWORLD_API_KEY=

# Required - Assembly.AI API key for speech-to-text
ASSEMBLY_AI_API_KEY=

# Required for greeting card message generation
ANTHROPIC_API_KEY=

# Required for image generation
GOOGLE_API_KEY=

# Optional - Voice cloning (uses INWORLD_API_KEY if not set)
INWORLD_PORTAL_API_KEY=

# Optional - LLM settings
LLM_MODEL_NAME=gemini-2.5-flash-lite
LLM_PROVIDER=google

# Optional - TTS settings
TTS_MODEL_ID=inworld-tts-1-max

# Optional - VAD model path (defaults to bundled model)
VAD_MODEL_PATH=

# Optional - Graph visualization (default: false)
GRAPH_VISUALIZATION_ENABLED=

# Optional - Disable auto-interruption (default: false)
DISABLE_AUTO_INTERRUPTION=
